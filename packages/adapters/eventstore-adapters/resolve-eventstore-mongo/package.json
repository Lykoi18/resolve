    "testw": "jest --config=../../../../jest.config.js --watchAll"
  },
  "repository": "https://github.com/reimagined/resolve.git",
  "author": "reimagined team",
  "license": "MIT",
  "keywords": [
    "cqrs",
    "eventsourcing",
    "es",
    "ddd"
  ],
  "homepage": "https://github.com/reimagined/resolve/tree/master/packages/adapters/eventstore-adapters/resolve-eventstore-mongo#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "mongodb": "^3.1.4",
